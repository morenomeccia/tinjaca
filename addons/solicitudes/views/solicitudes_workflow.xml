        <record model="workflow.transition" id="trans_asignar_requisitos_a_pendiente_requisitos">
            <field name="act_from" ref="activity_estacion_consignacion_requisitos"/>
            <field name="act_to" ref="activity_estatus_pendiente_requisitos"/>
            <field name="signal">button_pendiente_requisitos</field>
            <!-- <field name="group_id" ref="fomdes.group_informacion_credito"/> -->
        </record>

        <!-- Cambio de estacion (auto)" -->
        <record model="workflow.transition" id="trans_pendiente_requisitos_a_asignar_juridico">
            <field name="act_from" ref="activity_estatus_pendiente_requisitos"/>
            <field name="act_to" ref="activity_estacion_control_previo"/>
            <field name="condition">True</field>
            <!-- <field name="group_id" ref="fomdes.group_informacion_credito"/> -->
        </record>

